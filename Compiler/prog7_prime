#include <stdio.h>
blank main()
{
	varZ n;
	display("Enter n value:");
	scanZ(n);
	
	varZ i;
	varZ m;
	varZ m1;
	i=2;
	m=n/i;
	varZ k=0;
	for(i=2;i<m;i=i+1)
	{
		varZ k1;
		k1=n%i;
		if(k1==m1)
		{
			k=1;	
		}
		else
		{
		      
		}

	
	}
	m1=1;
	if(k==m1)
	{
		display(n);
		display(" is not a prime number");
	}
	else
	{
		display(n);
		display(" is  a prime number");
	
	}

display("\n");

}
